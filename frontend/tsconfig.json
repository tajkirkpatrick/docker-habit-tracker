{
    // "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "declaration": true,
        "emitDeclarationOnly": true,
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        // For SolidJS
        "jsx": "preserve",
        "jsxImportSource": "solid-js"
    }
}
